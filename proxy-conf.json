{
    "/api/*": {
        "target": "http://localhost:8081",
        "secure": false,
        "changeOrigin": true,
        "logLevel": "debug",
        "pathRewrite": {
            "^/api": "/api"
        }
    },
    "/file-api/*": {
        "target": "http://localhost:8080",
        "secure": false,
        "changeOrigin": true,
        "logLevel": "debug",
        "pathRewrite": {
            "^/file-api": "/api"
        }
    },
    "/pdfapi/*": {
        "target": "http://localhost:8082",
        "secure": false,
        "changeOrigin": true,
        "logLevel": "debug",
        "pathRewrite": {
            "^/pdfapi": "/api"
        }
    }
}